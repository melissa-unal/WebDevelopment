    <script>
        function f()
        {
          document.body.className= (document.body.className=="light") ? "dark" : "light";
          var buton_background = document.getElementById("toggle_body_background");

          if (buton_background.classList.contains("fa-sun"))
          {
            buton_background.classList.remove("fa-sun");
            buton_background.classList.add("fa-moon");
            return;
          }
          if (buton_background.classList.contains("fa-moon"))
          {
            buton_background.classList.remove("fa-moon");
            buton_background.classList.add("fa-sun");
            return;
          }
        }


    </script>

